<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rucafeproject5">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RUCafeProject5">
        <activity android:name=".DonutOrderActivity" android:parentActivityName=".DonutActivity" android:label="Select Donut(s) Quantity" ></activity>
        <activity android:name=".StoreActivity" android:parentActivityName=".MainActivity" android:label="View All Orders" />
        <activity android:name=".OrderActivity" android:parentActivityName=".MainActivity" android:label="View Shopping Cart"  />
        <activity android:name=".CoffeeActivity" android:parentActivityName=".MainActivity" android:label="Order Coffee(s)"  />
        <activity android:name=".DonutActivity" android:parentActivityName=".MainActivity" android:label="Order Donut(s)" />
        <activity android:name=".MainActivity" android:label="Rutgers Cafe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>